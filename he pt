	D = ( a1 * b2) - (a2 * b1);
	Dx = (c1 * b2) - ( b1 * c2);
	Dy = (a1 * c2) - ( a2 * c1);
	if ( D == 0) {
		if ( Dx + Dy == 0){
			printf ("Phuong trinh co vo so nghiem \n");
		}
		else {
			printf (" Phuong trinh vo nghiem \n");
		}
	}
	else {
		x = Dx / D;
		y = Dy / D;
		printf ("Phuong trinh co 2 nghiem phan biet (x,y) = (%f , %f ) \n",x,y);
