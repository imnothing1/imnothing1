#include<iostream>
using namespace std;

void nhap ( int &x , int &y) {
	cout << " Nhap gia tri x , y : " ;
	cin >> x >> y ;
}
void vehinhchunhat ( int x , int y) {
	for ( size_t i = 0 ; i < y ; i++){
		for ( size_t j = 0 ; j < x ; j ++){
			cout << " * " ;
		}
		cout << endl;
		
	}
}
int main (){
	int x,y;
	nhap ( x,y) ;
	vehinhchunhat ( x,y);
	
	return 0;
}
