#include<iostream>
using namespace std;

void nhap ( int &h){
	cout << " Nhap chieu cao h \n ";
	cin >> h ;
}
void veTamgiacv ( int &h){
	for ( size_t i = 0 ; i < h ; i ++){
		for ( size_t j = 0 ; j <= i ; j ++ ){
			cout << " * ";
		}
		cout << endl;
	}
}
int main () {
	int h ;
	nhap ( h) ;
	veTamgiacv ( h) ;
	return 0;
}
