#include<iostream>
using namespace std;

void nhap ( int &x , int &y ){
	cout << " Nhap gia tri duong x va y \n" ;
	cin >> x >> y;
}
void vehcn ( int &x , int &y){
	for ( size_t i = 0 ; i < y ; i ++) {
		for ( size_t j = 0 ; j < x ; i ++){
			if ( i== 0 || i == y - 1 || j == 0 || j == x -1 ){
				cout << " * " ;
			}
			else {
				cout << "   " ;
			}
		}
       cout << endl;
	}
}

int main(){
	int x , y ;
	nhap ( x ,y) ;
	vehcn ( x,y );
	return 0;
}
